<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{title}}</title>
    <link rel="stylesheet" href="/css/styles.css">
    
</head>

<body>
   {{> header}}  <!-- Partial del header -->
    
    <main>

         <div class="page-lead">
            {{#if pageLead.[0].image.src}}
                <img src="{{pageLead.[0].image.src}}" alt="{{pageLead.[0].alt}}">
            {{/if}}
        </div>
        <h1 class="headline">{{headline}}</h1>
        <p class="subHeadline">{{{subHeadline}}}</p>
        
        

        {{> carousel}}  <!-- Partial del carrusel -->
        {{> audioPlayer}}  <!-- Partial del reproductor de audio -->
        {{> socialLinks}}  <!-- Partial de redes sociales -->
    </main>

    {{> footer}}  <!-- Partial del footer -->
     <script src="/js/main.js"></script>

</body>

</html>